<h1>All Questions</h1>

<img src="http://www.sesamestreet.org/cms_services/services?action=download&uid=dbf13ec8-16cd-11dd-a1a2-3f408a4274b3" alt="" />

  <% @questions.each do |ques|  %>
  <div class="well">
  <%= link_to ques.title, question_path(ques) %>
  </div>
  <%end  %>

<ul class="pagination">
  <% (1..Question.num_of_pages).each do |page_num| %>
  <li> <%= link_to page_num, questions_path(page_num: page_num) %> </li>
  <% end %>
</ul>
